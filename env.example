# PromptInfra Configuration
# Copy this to .env and fill in your values

# OpenAI API Key (get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-your-openai-key-here

# Terraform Cloud Configuration (get from https://app.terraform.io/app/settings/tokens)
TF_API_TOKEN=your-terraform-cloud-token-here
TF_ORGANIZATION=your-org-name
TF_WORKSPACE=promptinfra-workspace

# Optional: AWS Region
AWS_REGION=us-east-1
